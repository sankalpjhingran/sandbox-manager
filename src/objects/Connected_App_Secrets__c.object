<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Protected custom settings to store the encrypted client id and secret for the connected app. This app will be used to authenticate other production orgs.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Client_Id__c</fullName>
        <description>Encrypted Client Id of the connected app</description>
        <externalId>false</externalId>
        <label>Client Id</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Client_Secret__c</fullName>
        <description>Encrypted Client secret for the connected app</description>
        <externalId>false</externalId>
        <label>Client Secret</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Connected App Secrets</label>
    <visibility>Protected</visibility>
</CustomObject>
